!_TAG_FILE_FORMAT	2	/extended format; --format=1 will not append ;" to lines/
!_TAG_FILE_SORTED	1	/0=unsorted, 1=sorted, 2=foldcase/
!_TAG_PROGRAM_AUTHOR	Darren Hiebert	/dhiebert@users.sourceforge.net/
!_TAG_PROGRAM_NAME	Exuberant Ctags	//
!_TAG_PROGRAM_URL	http://ctags.sourceforge.net	/official site/
!_TAG_PROGRAM_VERSION	5.8	//
DEFAULT	more.c	6;"	d	file:
DEFAULT	morePro.c	6;"	d	file:
MAXSIZE	more.c	5;"	d	file:
MAXSIZE	morePro.c	5;"	d	file:
getLineNum	more.c	/^int getLineNum()$/;"	f
getLineNum	more.c	/^int getLineNum();$/;"	p	file:
getLineNum	morePro.c	/^int getLineNum(FILE *);$/;"	p	file:	signature:(FILE *)
getLineNum	morePro.c	/^int getLineNum(FILE *fdin)$/;"	f	signature:(FILE *fdin)
main	more.c	/^int main(int argc, char *argv[])$/;"	f	signature:(int argc, char *argv[])
main	morePro.c	/^int main(int argc, char *argv[])$/;"	f	signature:(int argc, char *argv[])
showTheFile	more.c	/^void showTheFile(FILE *, int);$/;"	p	file:	signature:(FILE *, int)
showTheFile	more.c	/^void showTheFile(FILE *fd, int linenum)$/;"	f	signature:(FILE *fd, int linenum)
showTheFile	morePro.c	/^void showTheFile(FILE *, int);$/;"	p	file:	signature:(FILE *, int)
showTheFile	morePro.c	/^void showTheFile(FILE *fd, int linenum)$/;"	f	signature:(FILE *fd, int linenum)
